MainActivity.java
import android.support.v7.app.AppCompatActivity;
import android.os.Bundle;
import android.view.View;
import android.widget.Button;
import android.widget.TextView;
import java.util.HashSet;
public class MainActivity extends AppCompatActivity {
double val1,val2;
boolean add,sub,div,mul;
    TextView result;
    Button numone;
    Button ntwo;
    Button nthree;
    Button nfour;
    Button nfive;
    Button nsix;
    Button nseven;
    Button neight;
    Button nnine;
    Button nzero;
    Button bdot;
    Button badd;
    Button bsub;
    Button bmul;
    Button bdiv;
    Button bequ;
    Button bclear;
    @Override
protected void onCreate(Bundle savedInstanceState) {
super.onCreate(savedInstanceState);
setContentView(R.layout.activity_main);
result = (TextView) findViewById(R.id.txtScreen);
numone=(Button)findViewById(R.id.one);
ntwo=(Button)findViewById(R.id.two);
nthree=(Button)findViewById(R.id.thr);
nfour=(Button)findViewById(R.id.fou);
nfive=(Button)findViewById(R.id.fiv);
nsix=(Button)findViewById(R.id.six);
nseven=(Button)findViewById(R.id.sev);
neight=(Button)findViewById(R.id.eig);
nnine=(Button)findViewById(R.id.nin);
nzero=(Button)findViewById(R.id.zer);
bdot=(Button)findViewById(R.id.dot);
badd=(Button)findViewById(R.id.addbut);
bsub=(Button)findViewById(R.id.subbut);
bmul=(Button)findViewById(R.id.mulbut);
bdiv=(Button)findViewById(R.id.divbut);
bequ=(Button)findViewById(R.id.equbut);
bclear=(Button)findViewById(R.id.clearbut);


numone.setOnClickListener(new View.OnClickListener() {
            @Override
public void onClick(View view) {
result.setText(result.getText() + "1");
            }
        });
ntwo.setOnClickListener(new View.OnClickListener() {
            @Override
public void onClick(View view) {
result.setText(result.getText() + "2");
            }
        });
nthree.setOnClickListener(new View.OnClickListener() {
            @Override
public void onClick(View view) {
result.setText(result.getText() + "3");
            }
        });
nfour.setOnClickListener(new View.OnClickListener() {
            @Override
public void onClick(View view) {
result.setText(result.getText() + "4");
            }
        });
nfive.setOnClickListener(new View.OnClickListener() {
            @Override
public void onClick(View view) {
result.setText(result.getText() + "5");
            }
        });
nsix.setOnClickListener(new View.OnClickListener() {
            @Override
public void onClick(View view) {
result.setText(result.getText() + "6");
            }
        });
nseven.setOnClickListener(new View.OnClickListener() {
            @Override
public void onClick(View view) {
result.setText(result.getText() + "7");
            }
        });
neight.setOnClickListener(new View.OnClickListener() {
            @Override
public void onClick(View view) {
result.setText(result.getText() + "8");
            }
        });
nnine.setOnClickListener(new View.OnClickListener() {
            @Override
public void onClick(View view) {
result.setText(result.getText() + "9");
            }
        });
nzero.setOnClickListener(new View.OnClickListener() {
            @Override
public void onClick(View view) {
result.setText(result.getText() + "0");
            }
        });
bdot.setOnClickListener(new View.OnClickListener() {
            @Override
public void onClick(View view) {
result.setText(result.getText() + ".");
            }
        });
badd.setOnClickListener(new View.OnClickListener() {
            @Override
public void onClick(View view) {
                val1=Double.parseDouble(result.getText()+"");
add=true;
result.setText(null);
            }
        });
bsub.setOnClickListener(new View.OnClickListener() {
            @Override
public void onClick(View view) {
                val1=Double.parseDouble(result.getText()+"");
sub=true;
result.setText(null);
            }
        });
bdiv.setOnClickListener(new View.OnClickListener() {
            @Override
public void onClick(View view) {
                val1=Double.parseDouble(result.getText()+"");
div=true;
result.setText(null);
            }
        });
bmul.setOnClickListener(new View.OnClickListener() {
            @Override
public void onClick(View view) {
                val1=Double.parseDouble(result.getText()+"");
mul=true;
result.setText(null);
            }
        });
bequ.setOnClickListener(new View.OnClickListener() {
            @Override
public void onClick(View view) {
                val2=Double.parseDouble(result.getText()+"");
if (add==true) {
result.setText(val1+val2+"");
add=false;
                }
if (sub==true) {
result.setText(val1-val2+"");
sub=false;
                }
if (mul==true) {
result.setText(val1*val2+"");
mul=false;
                }
if (div==true) {
result.setText(val1/val2+"");
div=false;
                }
            }
        });
bclear.setOnClickListener(new View.OnClickListener() {
            @Override
public void onClick(View view) {
result.setText("");
            }
        });
    }
}

Activity_main.xml

